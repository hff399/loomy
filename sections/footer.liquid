                  <div class="d-flex align-items-center">
                    <div class="footer-currency-language-selector">
                      {% render 'localization-form', section: section %}
                    </div>
                  </div>
                {% endif %}
              {% endif %}
              {% if section.settings.show_copyright %}
                <div class="d-flex align-items-center">
                  <p class="copyright-text text-sm body-font font-normal">
                    Copyright&copy; {{ 'now' | date: '%Y' }}
                    {{ shop.name | link_to: routes.root_url }},
                    {{ powered_by_link -}}
                  </p>
                </div>
              {% endif %}
           </div>
            {% if section.settings.show_social_icons and social_icons %}
                <div class="footer-bottom-center-content">
                    <div class="footer-social-icons social-icons-style-{{ section.settings.social_icons_style }}">
                        {% render 'social-icons', section: section %}
                    </div>
                </div>
            {% endif %}
            {% if section.settings.payment_enable
              or shop.features.follow_on_shop?
              and section.settings.enable_follow_on_shop
            %}
              <div class="d-flex align-items-center gap-10">
                {% if section.settings.payment_enable %}
                  {% render 'payment-icons', section: section %}
                {% endif %}
                {%- if shop.features.follow_on_shop? and section.settings.enable_follow_on_shop -%}
                  <div class="footer-follow-on-shop">
                    {{ shop | login_button: action: 'follow' }}
                  </div>
                {%- endif -%}
              </div>
            {% endif %}

          </div>
        </div>
      </div>
    {% endif %}
    {% if section.settings.show_policy %}
