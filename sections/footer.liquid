        <div class="container">
          {% comment %} <div class="row"> {% endcomment %}
            {% if section.settings.show_policy %}
              {% comment %} <div class="col-12 col-md-12"> {% endcomment %}
                <div class="footer--info-block text-center">
                  <ul class="footer-quick-links list-inline">
                    {%- for policy in shop.policies -%}
                      {%- if policy != blank -%}
                        <li>
                          <a
                            href="{{ policy.url }}"
                            class="footer-menu-link text-sm"
                            aria-label="Quick menu link"
                          >
                            {{- policy.title -}}
                          </a>
                        </li>
                      {% endif %}
                    {% endfor %}
                  </ul>
                </div>
              {% comment %} </div> {% endcomment %}
            {% endif %}
          {% comment %} </div> {% endcomment %}
        </div>
      </div>
    {% endif %}
