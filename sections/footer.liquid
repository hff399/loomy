<script>
document.addEventListener("DOMContentLoaded", function () {
  const urlParams = new URLSearchParams(window.location.search);
  const formType = urlParams.get('form_type');
  const contactTags = urlParams.get('contact[tags]');
  const formId = window.location.hash.substring(1);
  const form = document.getElementById(formId);

  if (form && formType === "customer" && contactTags === "newsletter") {
    const successMessage = form.querySelector('[id^="Newsletter-success--"]');
    const errorMessage = form.querySelector('[id^="Newsletter-error--"]');

    if (!successMessage && !errorMessage) {
      const errorDiv = document.createElement("div");
      errorDiv.className = "form-message error";
      errorDiv.setAttribute("role", "status");
      errorDiv.textContent = "{{ 'newsletter.error' | t }}";

      form.appendChild(errorDiv);
    }
  }
});
</script>
<style>
.footer-section{
  --round-section-radius:{{ section.settings.top_border_radius }}px;
  --illustarted-colors-1:{{ section.settings.illustarted_colors_1 }};
  --illustarted-colors-2:{{ section.settings.illustarted_colors_2 }};
  --illustarted-colors-3:{{ section.settings.illustarted_colors_3 }};
  --illustarted-colors-4:{{ section.settings.illustarted_colors_4 }};
  --illustarted-colors-5:{{ section.settings.illustarted_colors_5 }};
}
{% for block in section.blocks %}
      {% if block.type == 'image-with-text' %}
        .footer-section {
          --footer-logo-width: {{ block.settings.logo_width }}px;
        }
      {% endif %}
    {% endfor %}
  @media only screen and (max-width: 767px) {
    {% for block in section.blocks %}
      {% if block.type == 'image-with-text' %}
        .footer-section {
          --footer-logo-width: {{ block.settings.logo_width_mobile }}px;
        }
      {% endif %}
    {% endfor %}
  }

</style>
