    <div class="footer-top-row{% if section.settings.footer_layouts == 'illustrated' %} footer-layout-illustrated{% endif %}">
      <div class="container">
         {% if section.settings.footer_layouts == 'illustrated' %}
         <div class="footer-layout-illustrated-container">
        {% endif %}
        <div class="footer-bottom-content d-flex"> 
            {% for block in section.blocks %}
              {%- case block.type -%}
              {%- when 'image-with-text' -%}
                {% if block.settings.footer_logo != blank or block.settings.footer_description != blank %}
                <div class="footer-columns" style="--column-width: {{ block.settings.column_width }}%">
                  <div class="footer-col footer-logo-block">
                    {%- if block.settings.footer_logo != blank -%}
                    <a
                      class="footer-logo"
                      href="{{ routes.root_url }}"
                      aria-label="{{ shop.name }}"
                    >
                      {%- assign image_2x_size = block.settings.logo_width | times: 2 | at_most: 2560 -%}
                        <img
                          class="footer-logo-img"
                          srcset="{{ block.settings.footer_logo | image_url: width: block.settings.logo_width }} 1x, {{ block.settings.footer_logo | image_url: width: image_2x_size }} 2x"
                          src="{{ block.settings.footer_logo | image_url: width: block.settings.logo_width }}"
                          alt="{{ block.settings.footer_logo.alt | default: shop.name | escape }}"
                          loading="lazy"
                        >
                    </a>
                  {% endif %}
                  {% if block.settings.footer_description != blank %}
                    <div class="footer-logo-block-description">
                      <div class="rte {{ block.settings.description_size }}">
                        {{ block.settings.footer_description | replace: '<table', '<div class="table-responsive"><table' | replace: '</table>', '</table></div>' }}
                      </div>
                    </div>
                  {% endif %}
                  </div>
                </div>
              {% endif %}

                {%- when 'newsletter' -%}
                  <div class="footer-columns" style="--column-width: {{ block.settings.column_width }}%">
                    <div class="footer-col footer-newsletter-wrap">
                      {% if block.settings.newsletter_heading != blank %}
                        <p class="footer-menu-title {{ block.settings.heading_font }} {{ block.settings.heading_size }}">
                          {%- capture markersPrefix -%}
                            <span class="markers-text {{ block.settings.markers_font }}">
                        {%- endcapture -%}
                        {%- capture markersSuffix -%}
                            </span>
                        {%- endcapture -%}
                          {{ block.settings.newsletter_heading | replace: '++', '<br/>' | replace: "[", markersPrefix | replace: "]", markersSuffix}}
                        </p>
                      {% endif %}
                      <div class="footer-newsletter">
                        {%- form 'customer', id: 'ContactFooter' %}
                          <input
                            type="hidden"
                            name="contact[tags]"
                            value="newsletter"
                          >
                          <div class="newsletter-form-group">
                            <label class="hidden" for="NewsletterForm--footer">{{ 'newsletter.label' | t }}</label>
                            <input
                              id="NewsletterForm--footer"
                              type="email"
                              name="contact[email]"
                              class="form-control"
                              value="{{ form.email }}"
                              aria-required="true"
                              autocapitalize="off"
                              autocomplete="email"
                              {%- if form.errors -%}
                                autofocus
                                aria-invalid="true"
                                aria-describedby="Newsletter-error-{{ section.id }}"
                              {%- elsif form.posted_successfully? -%}
                                aria-describedby="Newsletter-success-{{ section.id }}"
                              {%- endif -%}
                              placeholder="{% if  block.settings.placeholder_text != blank %}{{ block.settings.placeholder_text }}{% else %}{{ 'newsletter.label' | t }}{% endif %}"
                              pattern="^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)*[a-zA-Z]{2,}))$"
                              required
                            >
                            {% assign default_label = 'newsletter.button_label' | t %}
                            {% assign label = block.settings.button_text | default: default_label %}
                            <button type="submit" class="button button-sm buttonHover">
                              <span class="button-text">{{ label }}</span>
                            </button>
                          </div>
                          {%- if form.errors -%}
                            <div
                              class="form-message error"
                              id="Newsletter-error--{{ section.id }}"
                              tabindex="-1"
                              autofocus
                              role="status"
                            >
                              {{- form.errors.translated_fields.email | capitalize }}
                              {{ form.errors.messages.email -}}
                            </div>
                          {%- endif -%} 
                          {%- if form.posted_successfully? -%}
                            <div
                              id="Newsletter-success--{{ section.id }}"
                              class="form-message success"
                              tabindex="-1"
                              autofocus
                              role="status"
                            >
                              {{- 'newsletter.success' | t -}}
                            </div>
                          {%- endif -%}
                        {% endform %}
                      </div>
                    </div>
                  </div>
                {%- when 'menus' -%}
                  {% if block.settings.menu != blank %}
                    <div class="footer-columns" style="--column-width: {{ block.settings.column_width }}%">
                      <details
                        class="footer-col{% if section.settings.mobile_accordions %} footer-mb-accordions{% endif %}"
                        open
                        data-accordion
                      >
                        <summary class="footer-menu-title heading-font h6">
                          {% if block.settings.heading != blank %}
                            {{ block.settings.heading }}
                          {% else %}
                            {{ block.settings.menu.title }}
                          {% endif %}
                          {% if section.settings.mobile_accordions == true %}
                            <span class="accordion-icon d-lg-none d-md-none" data-accordion-icon>
                              {% render 'svg-icons', icon: 'accordion-icon' %}
                            </span>
                          {% endif %}
                        </summary>
                        <ul class="footer-menu-list" data-accordion-content>
                          {%- for link in block.settings.menu.links -%}
                            <li class="footer-menu-item">
                              <a
                                href="{{ link.url }}"
                                class="footer-menu-link text-sm"
                                aria-label="Menu link"
                              >
                                {{- link.title | escape -}}
                              </a>
                            </li>
                          {% endfor %}
                        </ul>
                      </details>
                    </div>
                  {% endif %}
