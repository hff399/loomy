  <div class="{{ containerClass }}">
    <div class="text-with-multiple-images-wrapper">
      {% if shape_block_count > 0 %}
        <shape-icons class="img-with-shapes-wrapper">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'shape' %}
                {% if block.settings.shape != 'none' %}
                  <div
                    class="shape-icon shape {{ block.settings.shape }}"
                    style="--shape-color: {{ block.settings.shape_color }}"
                    id="icon-shape-{{ block.id }}"
                    data-shape-icon>
                    <template>
                      {% render 'bg-shape'
                        , shape: block.settings.shape %}
                    </template>
                  </div>
                {% endif %}
            {% endcase %}
          {% endfor %}
        </shape-icons>
      {% endif %}

      <div class="row align-items-center image-position-{{ section.settings.desktop_image_postition }} enable-border-{{ section.settings.enable_border }}">
        {% if section.blocks.size > 0 %}
          <div class="col-xl-{{ desktop_image_width }} col-lg-{{ desktop_image_width }} col-md-6 col-12">
            <{% if settings.enable_card_animation %}animate-motion-list{% else %}div{% endif %} class="text-with-multiple-images-wrap{% if block_image_count <= 1 %} text-with-single-image-layout{% endif %} pos-relative d-block">
              {% if block_image_count > 0 %}
                {% for block in section.blocks %}
                  {% case block.type %}
                    {% when 'image' %}
                      {% assign image_index = '1' %}
                      {% liquid
                        capture current
                          cycle section.id: 1, 2, 3, 4
                        endcapture
                        assign cardImageSize = block.settings.image_ratio
                        if cardImageSize == 'landscape'
                          assign productImageRatio = '75'
                        elsif cardImageSize == 'square'
                          assign productImageRatio = '100'
                        elsif cardImageSize == 'portrait'
                          assign productImageRatio = '130'
                        elsif cardImageSize == 'auto'
                          if block.settings.image != blank
                            assign productImageRatio = 1 | divided_by: block.settings.image.aspect_ratio | times: 100
                          else
                            assign productImageRatio = '100'
                          endif
                        endif
                      %}
                      <div class="text-with-multiple-img-item" data-motion-list>
                        <div class="text-with-multiple-img-item-inner">
                          <div class="media media-overlay overlay-style-{{ section.settings.overlay_style }}" style="--image-ratio:{{ productImageRatio }}%;">
                            {% if block.settings.image != blank %}
                              {% render 'lazy-images'
                                , image: block.settings.image %}
                            {% else %}
                              {{ 'collection-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                            {% endif %}
                          </div>
                        </div>
                      </div>
                      {% assign image_index = image_index | plus: 1 %}
                  {% endcase %}
                {% endfor %}
              {% else %}
                <div class="text-with-multiple-img-item" data-motion-list>
                  <div class="text-with-multiple-img-item-inner">
                    <div class="media media-overlay overlay-style-{{ section.settings.overlay_style }}" style="--image-ratio:100%;">
                      {{ 'collection-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                    </div>
                  </div>
                </div>
              {% endif %}
            </{% if settings.enable_card_animation %}animate-motion-list{% else %}div{% endif %}>
          </div>

          <div class="col-xl-{{ desktop_content_width }} col-lg-{{ desktop_content_width }} col-md-6 col-12">
            <div class="image-with-text-content text-{{ section.settings.alignment }}">
              {% for block in section.blocks %}
                {% case block.type %}
                  {% when 'heading' %}
                    {% if block.settings.sub_heading != blank %}
                      <p class="body-font text sub-heading {% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="50">
                        {% endif %}
                        {{ block.settings.sub_heading }}
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </p>
                    {% endif %}

                    {% if block.settings.heading != blank %}
                      <{{ block.settings.seo_heading }} class="section-heading {{ block.settings.heading_font }} {{ block.settings.heading_size }}{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="50">
                        {% endif %}
                        {%- capture markersPrefix -%}
                            <span class="markers-text {{ block.settings.markers_font }}">
                          {%- endcapture -%}
                        {%- capture markersSuffix -%}
                            </span>
                          {%- endcapture -%}
                        {{- block.settings.heading | replace: '++', '<br/>' | replace: "[", markersPrefix | replace: "]", markersSuffix -}}
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </{{ block.settings.seo_heading }}>
                    {% endif %}
                  {% when 'text' %}
                    {% if block.settings.description != blank %}
                      <div class="section-description image-with-text-desc pt-0 rte {{ block.settings.description_size }}{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="100">
                        {% endif %}
                        {{ block.settings.description | replace: '<p></p>', '<br>' | replace: '<table', '<div class="table-responsive"><table' | replace: '</table>', '</table></div>' }}
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </div>
                    {% endif %}
                  {% when 'button' %}
                    {% if block.settings.button_text != blank %}
                      <div class="pt-0 section-buttons{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="150">
                        {% endif %}
                        {% render 'button-wrapper'
                          , block_settings: block
                          , button_style: block.settings.button_style
                          , button_text: block.settings.button_text
                          , button_link: block.settings.button_link
                          , button_size: block.settings.button_size %}
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </div>
                    {% endif %}
                  {% when 'divider' %}
                    <div class="divider divider-bottom"></div>
                {% endcase %}
              {% endfor %}
            </div>
          </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>
<style>
  #shopify-section-{{ section.id }} {
    --top-spacing: {{ section.settings.top_space_desktop }}px;
    --bottom-spacing: {{ section.settings.bottom_space_desktop }}px;
    --round-section-radius: {{ section.settings.corner_radius }}px;
    --animation-colors-opacity:{{ section.settings.animation_colors_opacity }};
  }
  @media only screen and (max-width: 767px) {
    #shopify-section-{{ section.id }} {
      --top-spacing: {{ section.settings.top_space_mobile }}px;
      --bottom-spacing: {{ section.settings.bottom_space_mobile }}px;
    }
  }

  {% for block in section.blocks %}
    {% case block.type %}
    {% when 'shape' %}
    #shopify-section-{{ section.id }} #icon-shape-{{ block.id }} {
      --shape-horizontal: {{ block.settings.shape_desktop_horizontal }}%;
      --shape-vertical: {{ block.settings.shape_desktop_vertical }}%;
      --shape-width: {{ block.settings.shape_width }}px;
    }
    @media only screen and (max-width: 767px) {
      #shopify-section-{{ section.id }} #icon-shape-{{ block.id }} {
        --shape-horizontal: {{ block.settings.shape_mobile_horizontal }}%;
        --shape-vertical: {{ block.settings.shape_mobile_vertical }}%;
        {% if block.settings.icon_visibility == 'large' %}
         display: none !important;
        {% endif %}
      }
    }
    {% if block.settings.icon_visibility == 'small' %}
        @media(min-width: 768px) {
          #shopify-section-{{ section.id }} #icon-shape-{{ block.id }} {
            display: none !important;
          }
        }
    {% endif %}
  {% endcase %}
{% endfor %}

{% if section.settings.section_visibility == 'large' %}
  @media(max-width: 767px) {
    #shopify-section-{{ section.id }} {
      display: none !important;
    }
  }
{% elsif section.settings.section_visibility == 'small' %}
@media(min-width: 768px) {
  #shopify-section-{{ section.id }} {
    display: none !important;
  }
}
{% endif %}
</style>
