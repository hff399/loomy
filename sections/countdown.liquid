<div class="container">

{% if section.settings.layout_style == 'horizontal' %}
<div class="countdown-horizontal-content-wrapper">
{% endif %}

  <div class="section-header">
    <div class="section-header-content-wrapper text-{{ section.settings.alignment }} m-auto">
      {% if section.settings.sub_heading != blank %}
        <p class="text-lg body-font">{{ section.settings.sub_heading }}</p>
      {% endif %}
      {% if section.settings.heading != blank %}
        <{{ section.settings.seo_heading }} class="section-heading {{ section.settings.heading_font }} {{ section.settings.heading_size }}{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
          {% if settings.enable_heading_animation %}
            <animate-element data-animate="fade-up-large" data-animate-delay="50">
          {% endif %}
            {%- capture markersPrefix -%}
              <span class="markers-text {{ section.settings.markers_font }}">
            {%- endcapture -%} 
            {%- capture markersSuffix -%}
              </span>
            {%- endcapture -%}
          {{- section.settings.heading | replace: '++', '<br/>' | replace: "[", markersPrefix | replace: "]", markersSuffix  -}}
          {% if settings.enable_heading_animation %}
            </animate-element>
          {% endif %}
        </{{ section.settings.seo_heading }}>
      {% endif %}
      {% if section.settings.description != blank %}
        <div class="section-description{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
          {% if settings.enable_heading_animation %}
            <animate-element data-animate="fade-up-large" data-animate-delay="100">
          {% endif %}
          <p class="{{ section.settings.description_size }}">
            {{ section.settings.description | newline_to_br }}
          </p>
          {% if settings.enable_heading_animation %}
            </animate-element>
          {% endif %}
        </div>
      {% endif %}
    </div>
  </div>

  {% if section.settings.months != blank and section.settings.day != blank and section.settings.year != blank and section.settings.hours != blank and section.settings.minutes != blank %}
    <div class="countdown-wrapper">
      <countdown-timer>
        <div class="countdown d-grid justify-content-center countdown-glass-effect-{{ section.settings.enable_glass_effect }} countdown-style-{{ section.settings.countdown_style }}" data-countdown-input="{{ launch_month }}/{{ launch_date }}/{{ launch_year }} {{ launch_hour }}:{{ launch_minute }}:00">
          <div class="countdown-box countdown-days" data-time-wraper="">
            <div class="countdown-value">
              <span class="countdown-unit-value heading-font h5 d-block" data-days="days">0</span>
            </div>
            <div class="countdown-unit">
              <span class="countdown-unit-text text-sm d-block">{{ 'sections.countdown.days' | t }}</span>
            </div>
          </div>
          <div class="countdown-box countdown-hours" data-time-wraper="">
            <div class="countdown-value">
              <span class="countdown-unit-value heading-font h5 d-block" data-hours="hours">0</span>
            </div>
            <div class="countdown-unit">
              <span class="countdown-unit-text text-sm d-block">{{ 'sections.countdown.hours' | t }}</span>
            </div>
          </div>
          <div class="countdown-box countdown-minutes" data-time-wraper="">
            <div class="countdown-value">
              <span class="countdown-unit-value heading-font h5 d-block" data-minutes="minutes">0</span>
            </div>
            <div class="countdown-unit">
              <span class="countdown-unit-text text-sm d-block">{{ 'sections.countdown.minutes' | t }}</span>
            </div>
          </div>
          <div class="countdown-box countdown-seconds" data-time-wraper="">
            <div class="countdown-value">
              <span class="countdown-unit-value heading-font h5 d-block" data-seconds="seconds">0</span>
            </div>
            <div class="countdown-unit">
              <span class="countdown-unit-text text-sm d-block">{{ 'sections.countdown.seconds' | t }}</span>
            </div>
          </div>
        </div>
        <div class="countdown-expired hidden countdown-box" data-expire-message>
          <p class="countdown-expired-text heading-font h5 d-block">
          {% if section.settings.time_expire != blank %}  
            {{ section.settings.time_expire }}
          {% else %}
            {{ 'sections.countdown.time_expire' | t }}
          {% endif %}
            </p>
        </div>
      </countdown-timer>
      {% if section.settings.button_text != blank and section.settings.button_link != blank %}
        <div class="countdown-button-wrap">
            {% render 'button-wrapper'
                ,block_settings: section
                ,button_style: section.settings.button_style
                ,button_text: section.settings.button_text
                ,button_link: section.settings.button_link
                ,button_size: section.settings.button_size
            %}
        </div>
      {% endif %}
    </div>
  {% endif %}
  {% if section.settings.layout_style == 'horizontal' %}
    </div>
  {% endif %}

  <div class="countdown-content-wrapper">
    {% if section.blocks.size > 0 %}
      <div class="countdown-product-card-wrapper">
        {% assign cardImageSize = section.settings.image_ratio %}
        {% for block in section.blocks %}
          {% if block.type == 'product' %}
            <swiper-slideshow>
              <div class="swiper" data-swiper='
                    {
                      "loop": false,
                      "speed": 600,
                      "spaceBetween": 20,
                      "slidesPerView":1,
                      "grabCursor": true,
                      "centerInsufficientSlides":false,
                      "centeredSlides": false,
                      "navigation": {
                        "enabled": true,
                        "nextEl": ".swiper-button-next-{{ section.id }}",
                        "prevEl": ".swiper-button-prev-{{ section.id }}"
                      },
                      "pagination": {
                        "el": ".swiper-pagination-{{ section.id }}",
                        "type": "bullets",
                        "clickable":true
                      },
                      "breakpoints": {
                        "320": {
                          "slidesPerView": 1.2
                        },
                        "767": {
                          "slidesPerView": 2
                        },
                        "992": {
                          "slidesPerView": 3
                        },
                        "1025": {
                          "slidesPerView": 3
                        },
                        "1200": {
                          "slidesPerView": {{ section.settings.desktop_column_count }}
                        }
                      }
                    }
                  '>
                <div class="countdown-product-list swiper-wrapper">
                  {% if block.settings.product_list != blank %}
                  {% for product in block.settings.product_list %}
                    {% liquid
                      if cardImageSize == 'landscape'
                        assign productImageRatio = '75'
                      elsif cardImageSize == 'square'
                        assign productImageRatio = '100'
                      elsif cardImageSize == 'portrait'
                        assign productImageRatio = '130'
                      else
                        if product.featured_media != blank
                          assign productImageRatio = 1 | divided_by: product.featured_media.preview_image.aspect_ratio | times: 100
                        else
                          assign productImageRatio = 100
                        endif
                      endif
                    %}
                    <div class="swiper-slide">
                      {% assign product_form_id = 'countdown-'
 | append: section.id
 | append: '-'
 | append: product.id
                      %}
                      {% render 'product-layout'
                        ,
 product: product
                        ,
 section: section
                        ,
 product_form_id: product_form_id
                        ,
 currentVariant: product.selected_or_first_available_variant
                        ,
 image_ratio: productImageRatio
                        ,
 quickview: true
                      %}
                    </div>
                  {% endfor %}
                {% else %}
{% for i in (1..5) %}
  {% liquid
          if cardImageSize == 'landscape'
            assign productImageRatio = '75'
          elsif cardImageSize == 'square'
            assign productImageRatio = '100'
          elsif cardImageSize == 'portrait'
            assign productImageRatio = '130'
          else
            assign productImageRatio = 100
          endif
        %}
  <div class="swiper-slide">
      {% render 'product-layout'
      ,product:''
      ,section: section
      ,image_ratio: productImageRatio
    %}
  </div>
{% endfor %}
              {% endif %}
                </div>
              </div>
              <div class="swiper--button-wrapper">
                <div class="slideshow-nav-btn slideshow-prev cursor-pointer swiper-button-prev-{{ section.id }} arrow-style-{{ settings.arrow_style }}">
                  {% if settings.arrow_style == 'star' %}
                    {% render 'svg-icons'
                      , icon: 'star-arrow-prev' %}
                  {% else %}
                    {% render 'svg-icons'
                      , icon: 'arrow-icon-prev' %}
                  {% endif %}
                </div>
                <div class="slideshow-nav-btn slideshow-next cursor-pointer swiper-button-next-{{ section.id }} arrow-style-{{ settings.arrow_style }}">
                  {% if settings.arrow_style == 'star' %}
                    {% render 'svg-icons'
                      , icon: 'star-arrow-next' %}
                  {% else %}
                    {% render 'svg-icons'
                      , icon: 'arrow-icon-next' %}
                  {% endif %}
                </div>
              </div>
            </swiper-slideshow>
        {% endif %}
        {% endfor %}
      </div>
    {% endif %}
  </div>
{% comment %} plane shape {% endcomment %}
</div>

