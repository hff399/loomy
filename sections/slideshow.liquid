{% if section.index <= 2 %}
  {{ 'slideshow.css' | asset_url | stylesheet_tag: preload: true }}
{% else %}
{{ 'slideshow.css' | asset_url | stylesheet_tag }}
{% endif %}


{% liquid
  if section.settings.section_container_width == 'global'
    assign containerClass = settings.container_width
  else
    assign containerClass = section.settings.section_container_width
  endif
%}
<div class="section-wrapper scheme-{{ section.settings.color_scheme }} section-spacing pos-relative section-{{ section.settings.section_background }} {% if section.settings.shape != 'none' %} section-shape-enable section-shape-{{ section.settings.shape_position }} shape-{{ section.settings.shape }}{% endif %}{% if section.settings.enable_sticky %} section-sticky-enable{% endif %} section-border-{{ section.settings.enable_border }}">
  <div class="{{ containerClass }}">
    <div class="slideshow-outer-wrapper">
      {% if section.blocks.size > 1 %}
        <swiper-slideshow>
          <div
            class="swiper navigation-{{ section.settings.carousel_navigation }} animation-{{ section.settings.animation_style }}"
            data-swiper='
              {
                "loop": true,
                {% if section.settings.animation_style == 'fade' %}
                "effect": "fade",
                {% elsif section.settings.animation_style == 'coverflow' %}
                "effect": "coverflow",
                "coverflowEffect": {
                    "rotate": 50,
                    "stretch": "50%",
                    "depth": 200,
                    "modifier": 1,
                    "slideShadows": true
                },
                {% elsif section.settings.animation_style == 'creative' %}
                "effect": "creative",
                    "creativeEffect": {
                    "prev": {
                        "shadow": true,
                        "translate": [0, 0, -400]
                    },
                    "next": {
                        "translate": ["100%", 0, 0]
                    }
                    },
                {% endif %}
                "speed": 600,
                "slidesPerView": 1,
                 "navigation": {
                    "enabled": true,
                    "nextEl": ".swiper-button-next-{{ section.id }}",
                    "prevEl": ".swiper-button-prev-{{ section.id }}"
                  },
                    "pagination": {
                            "el": ".swiper-pagination-{{ section.id }}",
                            "type": "bullets",
                            "clickable":true
                        },
                {% if section.settings.auto_rotate %}
                "autoplay":  {
                  "delay": {{ section.settings.change_slides_speed | times: 1000 }},
                  "disableOnInteraction": false
                },
                {% endif %}
                "grabCursor": true
              }
            '
          >
      {% endif %}
      <div class="slideshow-wrapper{% if section.blocks.size > 1 %} swiper-wrapper{% endif %}">
        {% for block in section.blocks %}
        
              <div
                id="slideshowItem-{{ block.id }}"
                class="{% if section.blocks.size > 1  %}swiper-slide {% endif %}slideshow-item pos-relative scheme-{{ block.settings.color_scheme }}"
                {{ block.shopify_attributes }}
              >
                {%- if block.settings.mobile_slide_image != blank and block.settings.slide_video == blank %}
                  <div class="slideshow-media media-wrapper d-block d-md-none media-overlay overlay-style-{{ block.settings.overlay_style }}">
                    <div class="media {% if section.settings.desktop_height == 'auto' %} d-media-adapt{% else %} d-media-fixed{% endif %}{% if section.settings.mobile_height == 'auto' %} m-media-adapt{% else %} m-media-fixed{% endif %}">
                      {% liquid
                        if block.settings.mobile_slide_image != blank
                          assign height = block.settings.mobile_slide_image.width | divided_by: block.settings.mobile_slide_image.aspect_ratio | round
                          assign sizes = '(min-width: 768px) 1440px, 100vw'
                          assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
                          if forloop.first
                            echo block.settings.mobile_slide_image | image_url: width: 3840 | image_tag: height: height, sizes: sizes, widths: widths, fetchpriority: 'high', loading: 'eager'
                          else
                            echo block.settings.mobile_slide_image | image_url: width: 3840 | image_tag: height: height, sizes: sizes, widths: widths, fetchpriority: 'low', loading: 'lazy'
                          endif
                        endif
                      %}
                    </div>
                  </div>
                {% endif %}

                <div class="slideshow-media media-wrapper{% if block.settings.mobile_slide_image != blank and block.settings.slide_video == blank %} d-none d-md-block{% endif %} media-overlay overlay-style-{{ block.settings.overlay_style }}">
                  <div class="media {% if section.settings.desktop_height == 'auto' %} d-media-adapt{% else %} d-media-fixed{% endif %}{% if section.settings.mobile_height == 'auto' %} m-media-adapt{% else %} m-media-fixed{% endif %}">
                    {% if block.settings.slide_video != blank %}
                      {{
                        block.settings.slide_video
                        | media_tag:
                          loading: 'lazy',
                          class: 'no-js-hidden slide-video',
                          autoplay: true,
                          loop: true,
                          mute: true,
                          controls: false,
                          preload: 'none'
                      }}
                    {% elsif block.settings.desktop_slide_image != blank %}
                      {% liquid
                        if block.settings.desktop_slide_image != blank
                          assign height = block.settings.desktop_slide_image.width | divided_by: block.settings.desktop_slide_image.aspect_ratio | round
                          assign sizes = '100vw'
                          assign widths = '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
                          if forloop.first
                            echo block.settings.desktop_slide_image | image_url: width: 3840 | image_tag: height: height, sizes: sizes, widths: widths, fetchpriority: 'high', loading: 'eager'
                          else
                            echo block.settings.desktop_slide_image | image_url: width: 3840 | image_tag: height: height, sizes: sizes, widths: widths, loading: 'lazy', fetchpriority: 'low'
                          endif
                        endif
                      %}
                    {% else %}
                      {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                    {% endif %}
                  </div>
                </div>

                <div class="slideshow-content-wrapper {{ section.settings.content_container }}">
                  <div class="slideshow-content-inner {{ block.settings.content_width }}-width {{ block.settings.desktop_content_position }} {{ block.settings.mobile_content_position }} d-text-{{ block.settings.desktop_text_alignment }} m-text-{{ block.settings.mobile_text_alignment }}">
                    {% if block.settings.heading != blank %}
                      <div class="slideshow-heading-wrap">
                        <{{ block.settings.seo_heading }} class="slideshow-heading {{ block.settings.heading_font }} {{ block.settings.heading_size }} {{ block.settings.heading_font_weight }}{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                          {% if settings.enable_heading_animation %}
                            <animate-element data-animate="fade-up-large" data-animate-delay="50">
                          {% endif %}
                          {%- capture markersPrefix -%}
                            <span class="markers-text {{ block.settings.markers_font }} {{ block.settings.heading_font_weight }}">
                          {%- endcapture -%}
                          {%- capture markersSuffix -%}
                            </span>
                          {%- endcapture -%}
                          {{- block.settings.heading | replace: '++', '<br/>' | replace: "[", markersPrefix | replace: "]", markersSuffix-}}
                          {% if settings.enable_heading_animation %}
                            </animate-element>
                          {% endif %}
                        </{{ block.settings.seo_heading }}>
                      </div>
                    {% endif %}
                    {% if block.settings.description != blank %}
                      <div class="section-description{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="100">
                        {% endif %}
                        <p class="{{ block.settings.description_size }}">
                          {{ block.settings.description | newline_to_br }}
                        </p>
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </div>
                    {% endif %}
                    {% if block.settings.button_text != blank and block.settings.button_link != blank %}
                      <div class="media-button-wrap{% if settings.enable_heading_animation %} animate-element-parent{% endif %}">
                        {% if settings.enable_heading_animation %}
                          <animate-element data-animate="fade-up-large" data-animate-delay="150">
                        {% endif %}
                        {% render 'button-wrapper',
                          block_settings: block,
                          button_style: block.settings.button_style,
                          button_text: block.settings.button_text,
                          button_link: block.settings.button_link,
                          button_size: block.settings.button_size, 
                        %}
                        {% if settings.enable_heading_animation %}
                          </animate-element>
                        {% endif %}
                      </div>
                    {% endif %}
                  </div>

                  {% if block.settings.thumnbnail_video != blank %}
                    <slide-thumbnail-video
                      class="slideshow-content-videothumnbnail"
                      data-aos-anchor-placement="bottom"
                      data-popup="video-{{ section.id }}"
                      data-video-url="{% if block.settings.thumnbnail_video.sources[0].url contains 'mp4' %}{{ block.settings.thumnbnail_video.sources[0].url -}}{% else %}{{ block.settings.thumnbnail_video.sources[1].url -}}{% endif %}"
                    >
                      <div class="slideshow-content-videothumnbnail-inner">
                        {%- liquid
                          assign poster = block.settings.thumnbnail_video.preview_image
                          assign cover_image = block.settings.video_thumnbnail_image
                          if cover_image != null
                            assign poster = cover_image
                          endif

                          if block.settings.image_ratio == 'landscape'
                            assign image_ratio = '75'
                          elsif block.settings.image_ratio == 'square'
                            assign image_ratio = '100'
                          elsif block.settings.image_ratio == 'portrait'
                            assign image_ratio = '120'
                          endif
                        -%}
                        {% if block.settings.enable_collapse %}
                          <div class="slide-video-collapse-wrapper" data-collapse-trigger tabindex="0">
                            <span class="minimize-icon">
                              <svg width="50" height="50" viewBox="0 0 584 584" fill="none">
                                <path d="M91.6664 0H491.666C515.978 0 539.294 9.65766 556.484 26.8487C573.675 44.0393 583.333 67.355 583.333 91.6667V291.667C583.333 298.297 580.699 304.657 576.011 309.343C571.322 314.033 564.963 316.667 558.333 316.667C551.703 316.667 545.344 314.033 540.655 309.343C535.967 304.657 533.333 298.297 533.333 291.667V91.6667C533.333 80.616 528.943 70.018 521.129 62.204C513.315 54.39 502.717 50 491.666 50H91.6664C80.6164 50 70.0164 54.39 62.2031 62.204C54.3897 70.018 49.9997 80.616 49.9997 91.6667V491.667C49.9997 502.717 54.3897 513.317 62.2031 521.13C70.0164 528.943 80.6164 533.333 91.6664 533.333H291.666C298.296 533.333 304.656 535.967 309.343 540.657C314.033 545.343 316.666 551.703 316.666 558.333C316.666 564.963 314.033 571.323 309.343 576.01C304.656 580.7 298.296 583.333 291.666 583.333H91.6664C67.3564 583.333 44.0397 573.677 26.8497 556.483C9.65637 539.293 -0.000305176 515.977 -0.000305176 491.667V91.6667C-0.000305176 67.355 9.65637 44.0393 26.8497 26.8487C44.0397 9.65766 67.3564 0 91.6664 0Z" fill="currentColor"/>
